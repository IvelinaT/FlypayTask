{
  "name": "FlypayTask",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "slim/slim": "^3.9",
    "slim/twig-view": "^2.1",
    "slim/csrf": "^0.8.1",
    "illuminate/database": "^5.4",
    "illuminate/events": "^5.4",
    "robmorgan/phinx":"^0.9",
    "respect/validation": "^1.1",
    "fzaninotto/faker": "^1.7",
    "illuminate/pagination":"~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Flyt\\": "src/App"
    }
  },
  "autoload-dev": {
    "classmap": ["src/App/database/"],
    "psr-4": {
      "Tests\\": "src/App/Tests/"
    }
  }
}

